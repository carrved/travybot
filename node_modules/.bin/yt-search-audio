#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/z4xi/Dev/Bots/travy bot/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/z4xi/Dev/Bots/travy bot/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir//bin/bash" ]; then
  exec "$basedir//bin/bash"  "$basedir/../yt-search/bin/mpv_audio.sh" "$@"
else
  exec /bin/bash  "$basedir/../yt-search/bin/mpv_audio.sh" "$@"
fi
